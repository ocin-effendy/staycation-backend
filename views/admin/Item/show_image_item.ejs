<div class="tab-pane fade show active" id="showImage" role="tabpanel" aria-labelledby="home-tab" tabindex="0">
    <!-- DataTales Example -->

    <div class="row">
        <% for(let i = 0; i < item.imageId.length; i++) { %>
        <div class="col-md-4">
            <div class="card shadow mb-4 mt-4">
            <div class="card-body">
                 <img src="http://localhost:3000/<%= item.imageId[i].imageUrl %>" class="img-fluid" alt="imageItem">
            </div>

        </div>
        </div>
        <% } %>
    </div>

    <a href="/admin/item" class="btn btn-primary">back</a>
       
</div>
